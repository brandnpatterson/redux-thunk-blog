{
  "name": "redux-thunk-blog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "react-app-rewired build && npm run create-cname",
    "create-cname": "echo 'redux-thunk-blog.surge.sh' > build/CNAME",
    "deploy": "npm run build && surge build",
    "start": "npm run stylelint && react-app-rewired start",
    "stylelint": "stylelint src/**/*.jsx && stylelint src/**/*.js",
    "test": "react-app-rewired test --env=jsdom"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "prop-types": "^15.5.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "react-moment": "^0.7.7",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.3.0",
    "styled-components": "^3.2.5"
  },
  "devDependencies": {
    "react-app-rewire-hot-loader": "^1.0.1",
    "react-app-rewired": "^1.5.2",
    "react-hot-loader": "^4.3.3",
    "stylelint": "^9.2.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1"
  }
}
